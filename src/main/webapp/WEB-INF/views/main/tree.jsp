</head>
<body>
<div id="wrap">
	<div id="jqxTree"></div>
	<div id="jqxWithTree"></div>
	<div id="groups"></div>
</div>
<script type="text/javascript">
	$(document).ready(function() 
	{
/* 
	$.ajax({
		datatype	:	"json",
		url			:	"/getGroupList",
		type		:	"post",
		success		:	function(data)
		{
			console.log(JSON.stringify(data));
		}
	});

	$.ajax({
		datatype	:	"json",
		url			:	"/getGroupParkList",
		data		:	{ "deptTy" : "G_1"},
		type		:	"post",
		success		:	function(data)
		{
			console.log(JSON.stringify(data));
		}
	});
		 */
      var adapter = new $.jqx.dataAdapter(
		{
			datatype	:	"json",
			url			:	"/getGroupList",
			dataFields	:	[
                { name: 'groupSeq', type: 'string' },
                { name: 'pGroupSeq', type: 'string' },
                { name: 'groupNm', type: 'string' },
                { name: 'dispOrdr', type: 'string' }
			],
			hierarchy	:	{
				keyDataField	:	{ name	:	"groupSeq" },
				parentDataField	:	{ name	:	"pGroupSeq" }
			},
			id			:	"groupSeq"
		});
		
		$("#jqxTree").jqxTreeGrid({
			source			:	adapter,
			width			:	"12%",
			height			:	"99.8%",
            altRows 		:	false,
            filterable		:	true,
            autoRowHeight	:	false,
            pageable		:	false,
            showHeader		:	false,
           	ready			:	function()
           	{
				$("#jqxTree").jqxTreeGrid("expandAll");
			},
            selectionMode	:	'singleRow',
			columns			:	[
								{ text	:	"명칭" , dataField	:	"groupNm" }
							]			
		});


       var _adapter = new $.jqx.dataAdapter(
		{
			datatype	:	"json",
			url			:	"/getGroupParkList",
			data		:	{ "deptTy" : "G_1"},
			dataFields	:	[
                { name: 'group_seq', type: 'string' },
                { name: 'p_group_seq', type: 'string' },
                { name: 'group_nm', type: 'string' },
                { name: 'disp_ordr', type: 'string' }
			],
			hierarchy	:	{
				keyDataField	:	{ name	:	"group_seq" },
				parentDataField	:	{ name	:	"p_group_seq" }
			},
			id			:	"group_seq"
		});

		$("#jqxWithTree").jqxTreeGrid({
			source			:	_adapter,
			width			:	"12%",
			height			:	"99.8%",
            altRows 		:	false,
            filterable		:	true,
            autoRowHeight	:	false,
            pageable		:	false,
            showHeader		:	false,
           	ready			:	function()
           	{
				$("#jqxWithTree").jqxTreeGrid("expandAll");
			},
            selectionMode	:	'singleRow',
			columns			:	[
								{ text	:	"명칭" , dataField	:	"group_nm" }
							]			
		});

 		$("#groups").jqxGrid(
		{
			source			:	new $.jqx.dataAdapter({
				datatype	:	"json",
				url			:	"/getCodeList",
				datafields	:	[
                    {name: 'cm_code_cd', type: 'string'},
                    {name: 'gr_code_cd', type: 'string'},
                    {name: 'gr_code_cd_nm', type: 'string'},
                    {name: 'sub_code_cd', type: 'string'},
                    {name: 'sub_code_cd_nm', type: 'string'},
                    {name: 'cm_code_cd_code', type: 'string'},
                    {name: 'cd_nm', type: 'string'},
                    {name: 'cd_nm_en', type: 'string'},
                    {name: 'cd_dc', type: 'string'},
                    {name: 'cd_dc_en', type: 'string'},
                    {name: 'use_at', type: 'string'},
                    {name: 'cd_del_at', type: 'string'},
                    {name: 'regist_trnsctor', type: 'string'},
                    {name: 'regist_trnsctor_nm', type: 'string'},
                    {name: 'regist_dt', type: 'string'},
                    {name: 'disp_ordr', type: 'number'},
				]
			}),	
			width			:	"100%",
			height			:	"99.8%",
			groupsexpandedbydefault: true,
			groupable		:	true,
			editable		:	false,			
			columns			: [
                { text : '코드', datafield : 'cm_code_cd', width:120, cellsalign: 'center'},
                { text : '대분류코드', datafield : 'gr_code_cd_nm', width:120, cellsalign: 'center'},
                { text : '중분류코드', datafield : 'sub_code_cd_nm', width:120, cellsalign: 'center'},
                { text : '소분류코드', datafield : 'cm_code_cd_code', width:120, cellsalign: 'center'},
                { text : '코드명', datafield : 'cd_nm', width:160, cellsalign: 'center'},
                { text : '코드명(영문)', datafield : 'cd_nm_en', width:160, cellsalign: 'center'},
                { text : '코드설명', datafield : 'cd_dc', width:230, cellsalign: 'center'},
                { text : '코드설명(영문)', datafield : 'cd_dc_en', width:230, cellsalign: 'center'},
                { text : '사용여부', datafield : 'use_at', width:80, cellsalign: 'center'},
                { text : '표시순서', datafield : 'disp_ordr', width:80, cellsalign: 'right'},
                { text : '삭제가능', datafield : 'cd_del_at', width:80, cellsalign: 'center'},
                { text : '등록 ID', datafield : 'regist_trnsctor', width:120, cellsalign: 'center'},
                { text : '등록일시', datafield : 'regist_dt', cellsformat: 'yyyy-MM-dd HH:mm', width: 230, cellsalign: 'center'}
			],
			groups	:	["gr_code_cd_nm", "sub_code_cd_nm"],
  			groupsrenderer	:	function(text, group, expanded, data)
			{
	             if (data.groupcolumn.datafields === 'gr_code_cd_nm') 
		             return '<div style="width: 100%; height: 100%; background-color: #37B8EF; ">' + text + '</div>';
                 else if (data.groupcolumn.datafields === 'sub_code_cd_nm') 
                     return '<div style="width: 100%; height: 100%; background-color: #47B14C; ">' + text + '</div>';
			}
		});
	});	
</script>

</body>
</html>



